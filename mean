mean1=38
wrongnumber=36
correctnumber=56
totalnumber=40
#sum of 40 numbers
sum=mean1*totalnumber
print("the sum of 40 number: ",sum)
#correct sum of these numbers
num2=sum-((wrongnumber)-(correctnumber))
print("sum-((wrongnumber)-(correctnumber)):"num2)
#the correct mean
mean2=num2/totalnumber
print(mean2)